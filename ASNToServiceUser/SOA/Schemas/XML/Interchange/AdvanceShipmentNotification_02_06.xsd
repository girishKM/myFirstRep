	<xs:import namespace="urn:rosettanet:specification:universal:PhysicalDimension:xsd:schema:01.07" schemaLocation="../Universal/PhysicalDimension_01_07.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Shared:PackageType:xsd:codelist:01.01" schemaLocation="../Domain/Shared/CodeList/RN_PackageType_01_01.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04" schemaLocation="../Universal/ProductIdentification_01_04.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Logistics:NationalExportControlClassification:xsd:codelist:01.03" schemaLocation="../Domain/Logistics/CodeList/RN_NationalExportControlClassification_01_03.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:UnitOfMeasure:xsd:codelist:01.05" schemaLocation="../Universal/CodeList/RN_UnitOfMeasure_01_05.xsd"/>
