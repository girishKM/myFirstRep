ation><xs:enumeration value="TES"><xs:annotation><xs:appinfo><urss:Definition>Test - The manufacturing stage in which the packaged die is tested.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration></xs:restriction></xs:simpleType><xs:complexType name="WorkInProcessLocationType"><xs:annotation><xs:appinfo><urss:CreationDate>2005-03-24</urss:CreationDate><urss:Identifier>WorkInProcessLocationType</urss:Identifier><urss:LastUpdatedDate>2005-03-24</urss:LastUpdatedDate><urss:RegisteredBy>RosettaNet</urss:RegisteredBy><urss:TypeVersion>01.01</urss:TypeVersion></xs:appinfo></xs:annotation><xs:simpleContent><xs:extension base="tns:WorkInProcessLocationContentType"><xs:attribute name="identifier" type="xs:token" fixed="WorkInProcessLocation"></xs:attribute><xs:attribute name="codeListVersion" type="xs:token" fixed="01.01"></xs:attribute><xs:attribute name="agency" type="xs:token" fixed="RosettaNet"></xs:attribute><xs:attribute name="schemaVersion" type="xs:token"></xs:attribute></xs:extension></xs:simpleContent></xs:complexType></xs:schema>
